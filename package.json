{
  "name": "frontend-performance-basecamp",
  "version": "0.0.1",
  "description": "frontend-performance-basecamp",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --node-env=production --env production",
    "build:dev": "webpack --node-env=development --env development",
    "build:prod": "webpack --node-env=production --env production",
    "watch": "webpack --watch",
    "serve": "webpack serve --node-env=development --env development",
    "stylelint": "stylelint --fix --cache '**/*.css'",
    "lint": "eslint --cache --fix **/*.{mjs,cjs,js,jsx} && npm run stylelint",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@giphy/js-fetch-api": "^4.1.1",
    "@react-icons/all-files": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@webpack-cli/generators": "^2.2.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "prettier": "2.3.2",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "lint-staged": {
    "*.{mjs,cjs,js,jsx}": "eslint --cache --fix",
    "*.{mjs,cjs,js,jsx,html,css,md,json}": "prettier --write",
    "*.{css,scss}": "stylelint --cache --fix"
  }
}
